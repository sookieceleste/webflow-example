{
  "project": {
    "src_path": "src",
    "build_path": "dist",
    "root_file_name": "index.html"
  },
  "links_replacements": [
    {
      "target": "\"index.html\"",
      "replace_by": "\"/\""
    },
    {
      "target": "\"perception.html\"",
      "replace_by": "\"/perception\""
    },
    {
      "target": "\"pinstripe.html\"",
      "replace_by": "\"/pinstripe\""
    },
    {
      "target": "\"fckpatriarchy.html\"",
      "replace_by": "\"/fckpatriarchy\""
    },
    {
      "target": "\"404.html\"",
      "replace_by": "\"/404\""
    }
  ],
  "file_links_replacement": [
    {
      "target": "\"css/",
      "replace_by": "\"../css/"
    },
    {
      "target": "images/",
      "replace_by": "../images/"
    },
    {
      "target": "\"js/",
      "replace_by": "\"../js/"
    }
  ],
  "transforms": [
    {
      "name": "index",
      "filepath": "index.html",
      "move_to_subfolder": false,
      "apply_links_replacements": true,
      "apply_file_links_replacements": false
    },
    {
      "name": "perception",
      "filepath": "perception.html",
      "move_to_subfolder": true,
      "apply_links_replacements": true,
      "apply_file_links_replacements": true
    },
    {
      "name": "pinstripe",
      "filepath": "pinstripe.html",
      "move_to_subfolder": true,
      "apply_links_replacements": true,
      "apply_file_links_replacements": true
    },
    {
      "name": "fckpatriarchy",
      "filepath": "fckpatriarchy.html",
      "move_to_subfolder": true,
      "apply_links_replacements": true,
      "apply_file_links_replacements": true
    },
    {
      "name": "404",
      "filepath": "404.html",
      "move_to_subfolder": true,
      "apply_links_replacements": true,
      "apply_file_links_replacements": true
    }
  ]
}
